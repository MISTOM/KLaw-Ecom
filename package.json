	"scripts": {
		"dev": "vite dev",
		"build": "npx prisma generate && node scripts/copy-pdf-worker.cjs && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"start": "node build/index.js",
		"drm:prep": "echo 'Placeholder (deprecated)';",
		"drm:encrypt": "npx tsc scripts/encrypt_doc.ts --outDir ./scripts --target ES2022 --module ESNext --moduleResolution bundler && node scripts/encrypt_doc.js"
	},
	"prisma": {
		"seed": "node prisma/seed.js"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^5.2.11",
		"@sveltejs/kit": "^2.17.0",
